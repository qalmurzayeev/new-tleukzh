<script id="form-handlers">
document.addEventListener('DOMContentLoaded', function() {
const webinarForm = document.getElementById('webinar-form');
const programForm = document.getElementById('program-form');
function showNotification(message, type = 'success') {
const notification = document.createElement('div');
notification.className = `fixed top-20 right-4 z-50 p-4 rounded-lg ${type === 'success' ? 'bg-secondary text-black' : 'bg-red-500 text-white'} font-semibold shadow-lg transform translate-x-full transition-transform duration-300`;
notification.textContent = message;
document.body.appendChild(notification);
setTimeout(() => {
notification.style.transform = 'translateX(0)';
}, 100);
setTimeout(() => {
notification.style.transform = 'translateX(100%)';
setTimeout(() => {
document.body.removeChild(notification);
}, 300);
}, 3000);
}
webinarForm.addEventListener('submit', function(e) {
e.preventDefault();
const formData = new FormData(this);
showNotification('Рақмет! Сіз біздің тегін вебинарыма тіркелдіңіз. Толық ақпаратты бүгін сізге айтамыз.');
this.reset();
});
programForm.addEventListener('submit', function(e) {
e.preventDefault();
const formData = new FormData(this);
showNotification('Өтінім сәтті жіберілді! Біздің команда 24 сағат ішінде сізбен байланысады.');
